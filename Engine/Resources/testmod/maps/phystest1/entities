class MyArcball {
	sceneNode exhibit {
		origin "0 2 0"
	}

	sceneCamera Cam {
		origin "0 2 2"
		range "500"
	}
	
	sceneLight lig {
		material "deferred/lightPositional"
		//treePosition "player_start/"
		diffuse "0.2 0.8 0.2 1"
		specular "0.8"
		range "2.5 2.5 2.5"
		origin "0 1 0"
		model "gen:cube width 5.0 height 5.0 depth 5.0"		// generera en boundingbox
	}
	
	controller {
		type arcball
		
		camera Cam
		control exhibit
	}
}

/*player_start extends MyArcball {
	sceneNode exhibit {
		model "models/detonics.md3 scale 0.2"
		material "detonics"
