box4 {
	sceneNode {
		model "gen:cube width 1 height 1 depth 1" 
		origin "0 4 0"
	}
	
	physBody {
		mass box {
			total 5.0
			dimensions "1.0 1.0 1.0"
		}
		
		gravity true
	}
	
	sceneLight {
		material "deferred/lightPositional"
		treePosition "box4/"
		diffuse "0.2 1.0 0.2 1.0"
		specular "1.0"
		model "gen:cube width 5.0 height 5.0 depth 5.0"
	}
	
	physGeom {
		type box
		
		dimensions "1 1 1"
	}
}

