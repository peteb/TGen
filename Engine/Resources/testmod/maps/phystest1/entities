light1 {
	sceneLight {
		material "deferred/lightDirectional"
		orientation "0 0.5 1"
		diffuse "0.1"
		specular "1.0"
	}
}

maincam {
	sceneCamera {
		origin "0 2 2"
		orientation "0 0 1"
		range "500"
	}

	sound {
		local {
			randomize {
				delay "direct"
				"sounds/sound1"
			}
		}
	}
}


player_start extends TheMachoGuy {
	sceneNode {
		material "Cube2"
		origin "0 2 0"
	}
	
	physBody {
		mass box {
			total 2.0
			dimensions "1 1 1"
		}
		
		gravity true
	}
	
	physGeom {
		type box
		
		dimensions "1 1 1"
	}
	
	sceneCamera HeadCam {
		origin "0 1 2"
		range "500"
		
		treePosition "player_start/"
	}
	
	controller {
		type ghost
		
		speed 1.0
		camera HeadCam
		control sceneNode
	}
	
	sceneLight {
		material "deferred/lightPositional"
		treePosition "player_start/"
		diffuse "1 1 0 1"
		specular "1.0"
		model "gen:cube width 5.0 height 5.0 depth 5.0"
	}
	
}
