ground2 {
	sceneNode {
		//model "gen:cube width '1.0' height '1.0' depth '1.0'" 
		//model "models/kub.md3 scale '1.0'"
		model "gen:cube width '20.0' height '20.0' depth '0.05'" 
		material "Cube"
		origin "0 -1 0"
		orientation "0.1 1 0"
	}
	
	physGeom {
		type plane
	
		orientation "0.1 1 0"

		distance "-1.0"
		friction 0.4
	}
}

ground3 {
	sceneNode {
		//model "gen:cube width '1.0' height '1.0' depth '1.0'" 
		//model "models/kub.md3 scale '1.0'"
		model "gen:cube width '20.0' height '20.0' depth '0.05'" 
		material "Cube"
		origin "0 -1 0"
		orientation "-0.1 1 0"
	}

	physGeom {
		type plane
	
		orientation "-0.1 1 0"
		distance "-1.0"
		friction 0.4
	}
}

ground4 {
	physGeom {
		type plane
		
		orientation "0 0 1"
		distance "-10.0"
		friction 0.4
	}
	
	sceneNode {
		//model "gen:cube width '1.0' height '1.0' depth '1.0'" 
		//model "models/kub.md3 scale '1.0'"
		model "gen:cube width '20.0' height '20.0' depth '0.05'" 
		material "Cube"
		origin "0 0 10"
		orientation "0 0 1"
	}
}

