

material nomat default 9 {
	lod 9 {
		pass fixed {
			front fill
			back fill

			color 1 0 0
		}
	}
}


material railgunMaterial default 9 {
	lod 9 {
		pass deferred/writeAttributesNoNMap {
			texunit 0 "railgun1.png" {
				sampler colorMap
			}

			texunit 1 "gen:fadeV size 512 start '0.1' end '0.9'" {
				sampler specularMap
			}
		}
	}
}
